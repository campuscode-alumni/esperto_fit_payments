<section class="page-section cta">
  <div class="container">
    <div class="row">
      <div class="col-xl-9 mx-auto">
          <h2 class="section-heading mb-4">
            <h2 class="section-heading mb-4">
              <span class="section-heading-upper ">Lan√ßar Aula Avulsa</span>
            </h2>
            <%= form_tag(search_single_class_registrations_path, method: "get") do %>
                <%= label_tag(:q, "Pesquisar Aluno:") %>
                <%= text_field_tag(:q) %>
                <%= submit_tag("Procurar") %>
              <% end %>
            <table>
              <thead>
                <th>Nome</th>
                <th>CPF</th>
              </thead>
            <% @registrations.each do |registration| %>
              <tr>
                <td><%=registration.name %></td>
                <td><%=registration.cpf%></td>
                <td><%= link_to 'Mostrar', registration_path(registration)%></td>
                <td><%= link_to 'Editar', edit_registration_path(registration)%></td>
              </tr>
            <% end %>
            </table>

          </h2>
          <p class="mb-0"></p>
        </div>
      </div>
    </div>
</section>
