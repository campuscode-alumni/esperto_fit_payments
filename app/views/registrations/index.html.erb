<table>
  <thead>
    <th>Nome</th>
    <th>CPF</th>
  </thead>
<%= @registrations.each do |registration| %>
  <tr>
    <td><%=registration.name %></td>
    <td><%=registration.cpf%></td>
    <td><%= link_to 'Mostrar', registration_path(registration)%></td>
    <td><%= link_to 'Editar', edit_registration_path(registration)%></td>
  </tr>
<% end %>

</table>