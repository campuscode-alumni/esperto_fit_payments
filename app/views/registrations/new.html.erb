<%= form_for @registrations do |f| %>
  <%= f.label :name, 'Nome' %>
  <%= f.text_field :name %>

  <%= f.label :cpf, 'CPF' %>
  <%= f.number_field :cpf %>

  <%= f.label :unity, 'Unidade' %>
  <%= f.text_field :unity %>

  <%= f.label :plan, 'Plano' %>
  <%= f.text_field :plan %>

  <%= f.submit 'Enviar' %>
<%end%>